.c_cont .p5 {
    background-color: #7fbe58;
    background-position: 10rpx center;
    background-repeat: no-repeat;
}

#result .detail .a_cont .con {
    font-size: 28rpx;
    font-weight: bold;
    text-align: center;
}

#result .detail .a_cont .con div {
    height: 42rpx;
    line-height: 42rpx;
    color: #666;
    padding: 0 26rpx 0 26rpx;
    position: relative;
}

#result .detail .a_cont .con div b {
    height: 24rpx;
    line-height: 24rpx;
    font-weight: 400;
    display: inline-block;
    padding-right: 2rpx;
}

#result .detail .a_cont .con div p {
    position: relative;
    height: 24rpx;
    background-color: #fff;
    border-radius: 10rpx;
    display: inline-block;
    width: 4rpx;
}

#result .detail .a_cont .con div p span {
    display: inline-block;
    position: absolute;
    height: 100%;
    border-radius: 10rpx;
    left: 0;
}

#result .detail .a_cont .con div strong {
    display: inline-block;
    padding-left: 2rpx;
}

#result .detail .a_cont .con div:nth-child(1) span {
    background-color: #fe2955;
    background-image: -webket-linear-gradient(90deg,#ff0844 0%,#ffb199 100%);
    background-image: linear-gradient(90deg,#ff0844 0%,#ffb199 100%);
}

#result .detail .a_cont .con div:nth-child(2) span {
    background-color: #08b05b;
    background-image: -webket-linear-gradient(-90deg,#2af598 0%,#009a45 100%);
    background-image: linear-gradient(-90deg,#2af598 0%,#009a45 100%);
}

#result .detail .a_cont .con div:nth-child(3) span {
    background-color: #fca483;
    background-image: linear-gradient(-90deg,#f6d365 0%,#fda085 100%);
    background-image: linear-gradient(-90deg,#f6d365 0%,#fda085 100%);
}

#result .detail .a_cont .con div:nth-child(4) span {
    background-color: #3ebbfe;
    background-image: -webket-linear-gradient(to right,#4facfe 0%,#00f2fe 100%);
    background-image: linear-gradient(to right,#4facfe 0%,#00f2fe 100%);
}

#result .detail .a_cont .con div:nth-child(5) span {
    background-color: #8443df;
    background-image: -webket-linear-gradient(90deg,#7028e4 0%,#e5b2ca 100%);
    background-image: linear-gradient(90deg,#7028e4 0%,#e5b2ca 100%);
}

#result .detail .b_cont .con b {
    color: #333;
    font-size: 24rpx;
}

#result .detail .b_cont .con span {
    font-weight: bold;
}

#invite_friend_link {
    display: inline-block;
}

