.luckTip {
    display: flex;
}

.luckTip>view {
    width: 50%;
}

.luckTip.box>view._left {
    width: 56%;
}

.luckTip.box>view._right {
    width: 44%;
}

.luckTip.dp {
    display: block;
    width: 100%;
}

.luckTip .list {
    display: flex;
    margin-bottom: 14rpx;
    align-items: center;
    height: 40rpx;
    line-height: 40rpx;
    justify-content: flex-start;
    width: 100%;
}

.luckTip .list .title {
    font-weight: bold;
    margin-right: 16rpx;
    font-size: 28rpx;
}

.luckTip .list .text {
    font-size: 26rpx;
    height: 100%;
    display: flex;
    flex: 1;
    white-space: nowrap;
}

.luckTip .list .text .star {
    margin-right: 2rpx;
    display: flex;
    align-items: center;
}

.luckTip .list .text .star image {
    width: 28rpx;
    height: 28rpx;
    margin-top: -3rpx;
}

.adContainer {
    position: relative;
    z-index: 1;
    width: 100%;
    padding: 40rpx 20rpx 0rpx;
}

.adContainer video {
    width: 100%;
}