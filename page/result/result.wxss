#result {
    padding-bottom: 80px;
}

#result .detail {
    font-size: 28rpx;
    width: 94%;
    height: auto;
    margin: 0 auto;
}

#result .detail .top_box {
    width: 100%;
    height: auto;
    padding: 50rpx 0;
    background-size: cover;
    font-size: 0;
}

#result .detail .main {
    background: #ffffff;
    border-radius: 8rpx;
}

#result .detail .top_box {
    text-align: center;
    padding: 20rpx 0rpx 40rpx;
}

#result .detail .top_box .astro {
    display: inline-block;
    vertical-align: middle;
    width: 150rpx;
    height: 150rpx;
    line-height: 150rpx;
}

#result .detail .top_box .astro .imgBox {
    width: 150rpx;
    height: 150rpx;
    border-radius: 100rpx;
    overflow: hidden;
    background: #ffffff;
}

#result .detail .top_box .astro image {
    width: 150rpx;
    height: 150rpx;
    margin: 0 auto;
    display: inline-box;
    vertical-align: middle;
}

#result .detail .top_box .astro image.headgear {
    position: absolute;
    width: 180rpx;
    height: 180rpx;
    top: -24rpx;
    left: -14rpx;
}

#result .detail .top_box .astro em {
    height: 5rpx;
    display: inline-block;
    padding: 0 3rpx;
    border-radius: 8rpx;
    line-height: 5rpx;
    color: #fff;
    font-style: normal;
    font-size: 24rpx;
}

#result .detail .top_box .astro em i {
    float: right;
    background: url(../images/arrow.png-do-not-use-local-path-./page/result/result.wxss&73&14) no-repeat center;
    margin: 0.14rpx 0 0 2rpx;
    background-size: 100%;
    width: 0.25rpx;
    height: 0.25rpx;
    display: inline-block;
    display: none;
}

#result .detail .top_box .top_desc {
    color: #333;
    line-height: 6rpx;
    text-align: center;
    font-size: 24rpx;
    font-weight: 500;
    display: inline-block;
    text-align: left;
    vertical-align: middle;
    padding-left: 2rpx;
    font-weight: normal;
    line-height: normal;
    margin-left: 20rpx;
}

#result .detail .top_box .top_desc .t1 {
    display: block;
    line-height: 1;
    font-size: 34rpx;
    font-weight: bold;
    color: #333333;
    padding-bottom: 10rpx;
}

#result .detail .top_box .top_desc .t2 {
    font-size: 24rpx;
}

#result .detail .con {
    color: #333;
    padding: 30rpx 40rpx;
}

#result .detail .con strong {
    display: block;
    color: #689ce2;
    font-size: 24rpx;
}

#result .detail .con p {
    margin-bottom: 24rpx;
}

#result .detail .con p span {
    line-height: 1.6;
}

#result .detail .line {
    width: 100%;
    height: 1px;
    display: block;
    border: 1rpx solid #f2f2f2;
    margin: 16rpx 0rpx 40rpx;
}

.invite_btn {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    text-align: center;
    margin: 0 auto;
    z-index: 2;
}

.invite_btn button {
    font-size: 28rpx;
    background-color: #ff8c8c;
    border-radius: 0rpx;
    padding: 30rpx 60rpx;
    color: #ffffff;
    display: block;
    line-height: normal;
    font-weight: bold;
}

.invite_btn button:after {
    display: none;
}

#result .detail .con .c_cont {
    display: none;
}

#result .detail .con .c_cont.on {
    display: block;
}

#result .detail .con .c_cont .luckMain>view {
    margin-bottom: 24rpx;
    font-size: 28rpx;
}

#result .detail .con .c_cont .luckMain>view strong {
    color: #fff;
    display: inline-block;
    vertical-align: middle;
    line-height: normal;
}

#result .detail .con .c_cont .luckMain>view .span_title image {
    width: 28rpx;
    height: 28rpx;
    display: inline-block;
    vertical-align: middle;
    margin-right: 4rpx;
}

#result .detail .con .c_cont .luckMain>view .span_title {
    font-size: 0;
    border-radius: 8rpx;
    color: #fff;
    display: inline-block;
    padding: 8rpx 12rpx;
    margin-right: 10rpx;
    background-size: 30rpx 30rpx;
    line-height: 34rpx;
    vertical-align: middle;
    margin-top: -4rpx;
}

#result .detail .con .c_cont .luckMain>view text {
    line-height: 1.8;
}

.c_cont .p1 {
    background-color: #80bee4;
    background-position: 10rpx center;
    background-repeat: no-repeat;
}

.c_cont .p2 {
    background-color: #ea749e;
    background-position: 10rpx center;
    background-repeat: no-repeat;
}

.c_cont .p3 {
